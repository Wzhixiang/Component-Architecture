<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wzx.issue">

    <application
        android:name=".TaskApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/task_app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">

        <activity android:name=".task.TasksActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.wzx.issue.add.AddTaskActivity" />

        <activity android:name="com.wzx.issue.selectExecutor.SelectExecutorActivity" />

    </application>

</manifest>
